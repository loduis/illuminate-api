{
    "name": "loduis/alegra-php",
    "description": "Illuminate Api",
    "keywords": [
        "alegra",
        "api"
    ],
    "homepage": "http://developer.alegra.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Loduis Madariaga",
            "email": "loduis@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Illuminate\\": "src/"
        }
    },
    "require": {
        "php": ">=5.6.11",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/support": "^5.4",
        "nesbot/carbon": "~1.21"
    },
    "require-dev": {
        "phpunit/phpunit": "5.6.*",
        "mockery/mockery": "^0.9.4"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
