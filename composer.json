{
    "name": "loduis/illuminate-api",
    "description": "Illuminate Api",
    "keywords": [
        "illuminate",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Loduis Madariaga",
            "email": "loduis@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Illuminate\\": "src/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/support": "^8|^9",
        "nesbot/carbon": "^2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9|^10",
        "mockery/mockery": "dev-master"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
